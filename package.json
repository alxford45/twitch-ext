{
  "name": "twitch-ext",
  "version": "0.0.1",
  "description": "Prototype for twitch extension to overlay Runelite loot tracker",
  "scripts": {
    "start": "npm run server & npm run web",
    "server": "webpack -w --config config/webpack.config.ts --config-name server --display-error-details",
    "web": "webpack-dev-server --config config/webpack.config.ts --config-name web",
    "docs": "typedoc ./src/server "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alxford45/twitch-ext"
  },
  "keywords": [
    "prototype, Runelite, twitch, extension, loot, tracker"
  ],
  "author": "Alex Ford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alxford45/twitch-ext/issues"
  },
  "homepage": "https://github.com/alxford45/twitch-ext#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^12.6.2",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/ws": "^6.0.2",
    "babel-loader": "^8.0.6",
